package fit;

public class Customer {
	private int id = 0;
    private String name;
	
	
	Customer(int id, String name) {
	    this.id = id;
	    this.name=name;
	}
	
	public String getName() {
		return this.name;
	}
	
	public int getId() {
		return this.id;
	}
	
	@Override
	public boolean equals (Object o) {
	    Customer c = (Customer) o;
	    if (c.id==this.id) {
	    	return true;
	    } else {
	    	return false;
	    }
	}
}
